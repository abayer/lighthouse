git:
  kind: github
  name: github
  server: https://github.com

# the secret used for webhooks
hmacToken: $HMACTOKEN

# Default values for Go projects.
# This is a YAML-formatted file.
# Declare variables to be passed into your templates.
image:
  tag: $VERSION

user: $BOTUSER

oauthToken: $BOTSECRET

engines:
  jx: false
  tekton: true

createIngress: true

domainName: $DOMAINNAME.nip.io
